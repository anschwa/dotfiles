[color]
    ui = true
[alias]
    ll = log --graph --pretty=format:\"%C(yellow)%h %C(blue)%ad%C(red)%d %C(reset)%s%C(green) [%cn]\" --decorate --date=short
    undo = reset --soft HEAD^
    st = status -sb --ignore-submodules
    ci = commit
    br = branch
    co = checkout
    df = diff
    review = diff --cached
    ignorechanges = update-index --assume-unchanged
[core]
    editor = "vi"
    excludesfile = ~/.gitignore_global
[push]
    default = current
    followTags = true
[pull]
    default = current
[fetch]
    default = matching
[status]
    showUntrackedFiles = all
[diff]
    wsErrorHighlight = all
[init]
	defaultBranch = main
